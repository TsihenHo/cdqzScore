<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="me.tsihen.cdqzScore">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.成都七中成绩查询"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".QuestionAnswerActivity"
            android:exported="false"
            android:label="@string/title_activity_question_answer"
            android:theme="@style/Theme.成都七中成绩查询" />
        <activity
            android:name=".QueryStepFourActivity"
            android:exported="false"
            android:label="@string/title_activity_query_step_four"
            android:theme="@style/Theme.成都七中成绩查询" />
        <activity
            android:name=".QueryStepThreeActivity"
            android:exported="false"
            android:label="@string/title_activity_query_step_three"
            android:theme="@style/Theme.成都七中成绩查询" />
        <activity
            android:name=".QueryStepTwoActivity"
            android:exported="false"
            android:label="@string/title_activity_query_step_two"
            android:theme="@style/Theme.成都七中成绩查询" />
        <activity
            android:name=".QueryStepOneActivity"
            android:exported="false"
            android:label="@string/title_activity_quey_step_one"
            android:theme="@style/Theme.成都七中成绩查询" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.成都七中成绩查询" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.成都七中成绩查询">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>